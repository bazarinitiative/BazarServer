{
    "AntiSpam": {
        //max count of user command base on ip.
        //format: "max_count, seconds"
        "Client.Command": [ "20, 5", "200, 100" ],
        //max count of user query base on ip
        "Client.Query": [ "150, 5" ]
    },

    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*"
}
